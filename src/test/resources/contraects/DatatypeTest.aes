contract DatatypeTest =

    record company =
      {  ceo: address,
         shareholders: map(address, int),
         employees: list(employee),
         location: companyAddress }
    
    record companyAddress =
      {  zip: int,
         street: string,
         city: string }

    record employee =
      {  address: address,
         firstname: string,
         lastname: string }
         
    entrypoint testString(varA: string, varB: int) : string =
        String.concat(varA, Int.to_str(varB))